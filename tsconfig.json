{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "allowJs": false,
    "resolveJsonModule": true,
    "lib": ["es2017", "dom", "esnext"],
    "experimentalDecorators": true,
    "jsx": "react"
  },
  "include": ["src", "database"]
}
