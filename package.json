{
  "name": "@omscentral/server",
  "description": "omscentral web server",
  "license": "ISC",
  "author": "Mehmet Bajin <mehmet.bajin@gatech.edu>",
  "homepage": "https://github.com/OMSCentral/omscentral-server",
  "repository": "github:OMSCentral/omscentral-server",
  "version": "0.0.0",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "run-s clean generate build:ts tslint",
    "build:ts": "tsc",
    "clean": "rimraf dist coverage",
    "format": "run-s format:database format:src format:package",
    "format:database": "prettier --write database/",
    "format:package": "prettier-package-json --write ./package.json",
    "format:src": "prettier --write src/",
    "generate": "VERBOSE=true graphql-codegen --config codegen.yml",
    "lint": "eslint src/ database/ --ext .js,.ts",
    "migrate:latest": "knex migrate:latest --knexfile dist/database/knexfile.js 2>&1",
    "migrate:rollback": "knex migrate:rollback --knexfile dist/database/knexfile.js",
    "seed:run": "knex seed:run --knexfile dist/database/knexfile.js",
    "serve:node": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/src/server.js",
    "start": "npm run migrate:latest && npm run serve:node",
    "test": "run-s test:lint test:unit test:e2e",
    "test:e2e": "jest test/e2e --runInBand --passWithNoTests",
    "test:unit": "jest test/unit --passWithNoTests",
    "ts-node": "ts-node",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "npm run clean && concurrently -k -p \"[{name}]\" -n \"typescript,graphql,node\" -c \"cyan.bold,magenta.bold,green.bold\" \"npm:watch:ts\" \"npm:watch:generate\" \"wait-on file:dist/src/server.js && npm run watch:node\"",
    "watch:debug": "npm run clean && concurrently -k -p \"[{name}]\" -n \"typescript,graphql,node\" -c \"cyan.bold,magenta.bold,green.bold\" \"npm:watch:ts\" \"npm:watch:generate\" \"wait-on file:dist/src/server.js && npm run watch:node-debug\"",
    "watch:generate": "chokidar 'src/api/schema.graphql' -c 'npm run generate'",
    "watch:node": "nodemon dist/src/server.js",
    "watch:node-debug": "nodemon --inspect dist/src/server.js",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "@hapi/boom": "9.1.0",
    "@hapi/joi": "17.1.1",
    "@sentry/node": "5.17.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "connect-session-knex": "1.7.1",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "express-session": "1.17.1",
    "firebase-admin": "8.12.1",
    "graphql": "14.x",
    "graphql-middleware": "4.0.2",
    "graphql-shield": "7.3.2",
    "graphql-tools": "6.0.9",
    "html-minifier": "4.0.0",
    "knex": "0.21.1",
    "lodash": "4.17.15",
    "lusca": "1.6.1",
    "moment": "2.26.0",
    "morgan": "1.10.0",
    "objection": "2.2.0",
    "objection-db-errors": "1.1.2",
    "objection-graphql": "0.4.5",
    "objection-visibility": "1.0.0",
    "pg": "8.2.1",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.3",
    "@graphql-codegen/introspection": "1.15.3",
    "@graphql-codegen/typescript": "1.15.3",
    "@graphql-codegen/typescript-document-nodes": "1.15.3",
    "@graphql-codegen/typescript-resolvers": "1.15.3",
    "@types/bluebird": "3.5.32",
    "@types/body-parser": "1.19.0",
    "@types/boom": "7.3.0",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.6",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.6",
    "@types/express-session": "1.17.0",
    "@types/graphql": "14.5.0",
    "@types/hapi__joi": "17.1.2",
    "@types/html-minifier": "3.5.3",
    "@types/jest": "26.0.0",
    "@types/lodash": "4.14.155",
    "@types/lusca": "1.6.2",
    "@types/morgan": "1.9.1",
    "@types/node": "14.0.13",
    "@types/pg": "7.14.3",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.46",
    "@types/winston": "2.4.4",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
