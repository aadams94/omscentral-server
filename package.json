{
  "name": "@omscentral/server",
  "description": "omscentral web server",
  "license": "ISC",
  "author": "Mehmet Bajin <mehmet.bajin@gatech.edu>",
  "homepage": "https://github.com/OMSCentral/omscentral-server",
  "repository": "github:OMSCentral/omscentral-server",
  "version": "0.0.0",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "run-s clean build:ts tslint",
    "build:ts": "tsc",
    "clean": "rimraf dist coverage",
    "format:package": "prettier-package-json --write ./package.json",
    "migrate:latest": "knex migrate:latest --knexfile dist/database/knexfile.js 2>&1",
    "migrate:rollback": "knex migrate:rollback --knexfile dist/database/knexfile.js",
    "seed:run": "knex seed:run --knexfile dist/database/knexfile.js",
    "serve:node": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/src/server.js",
    "start": "npm run migrate:latest && npm run serve:node",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "test:watch": "npm run test -- --watch",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "npm run clean && concurrently -k -p \"[{name}]\" -n \"typescript,node\" -c \"cyan.bold,green.bold\" \"npm:watch:ts\" \"wait-on file:dist/src/server.js && npm run watch:node\"",
    "watch:debug": "npm run clean && concurrently -k -p \"[{name}]\" -n \"typescript,node\" -c \"cyan.bold,green.bold\" \"npm:watch:ts\" \"wait-on file:dist/src/server.js && npm run watch:node-debug\"",
    "watch:node": "nodemon dist/src/server.js",
    "watch:node-debug": "nodemon --inspect dist/src/server.js",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "@elastic/app-search-node": "7.5.0",
    "@hapi/joi": "17.1.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "compression": "1.7.4",
    "connect-session-knex": "1.5.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "express-session": "1.17.0",
    "firebase-admin": "8.9.2",
    "graphql": "14.6.0",
    "html-minifier": "4.0.0",
    "knex": "0.20.8",
    "lodash": "4.17.15",
    "lusca": "1.6.1",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "objection": "2.1.2",
    "objection-db-errors": "1.1.2",
    "objection-graphql": "0.4.5",
    "objection-visibility": "0.4.0",
    "pg": "7.18.0",
    "request": "2.88.0",
    "request-promise": "4.2.5",
    "uuid": "3.4.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.29",
    "@types/body-parser": "1.17.1",
    "@types/boom": "7.3.0",
    "@types/compression": "1.0.1",
    "@types/cors": "2.8.6",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.2",
    "@types/express-session": "1.15.16",
    "@types/graphql": "14.5.0",
    "@types/hapi__joi": "16.0.8",
    "@types/html-minifier": "3.5.3",
    "@types/jest": "25.1.1",
    "@types/lodash": "4.14.149",
    "@types/lusca": "1.6.1",
    "@types/morgan": "1.7.37",
    "@types/node": "13.5.2",
    "@types/pg": "7.14.1",
    "@types/request": "2.48.4",
    "@types/request-promise": "4.1.45",
    "@types/uuid": "3.4.7",
    "@types/winston": "2.4.4",
    "@types/yargs": "15.0.2",
    "concurrently": "5.1.0",
    "cross-env": "7.0.0",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "prettier-package-json": "2.1.3",
    "rimraf": "3.0.1",
    "supertest": "4.0.2",
    "supertest-session": "4.0.0",
    "ts-jest": "25.0.0",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.7.5",
    "wait-on": "4.0.0",
    "yargs": "15.1.0"
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
