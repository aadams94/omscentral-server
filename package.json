    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "supertest-session": "4.0.0",
    "ts-jest": "26.1.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
